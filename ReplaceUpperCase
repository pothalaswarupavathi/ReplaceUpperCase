import java.util.*;
class ReplaceUppaerCase {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        String s=in.nextLine();
        String t="";
        for(int i=0;i<s.length();i++){
            if(s.charAt(i) >= 'A' && s.charAt(i)<= 'Z'){
                t=t+"#";
            }
            else{
                t=t+s.charAt(i);
            }
        }
        System.out.println(t);
    }
}
